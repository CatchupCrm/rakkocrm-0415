{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.9",
    "caffeinated/flash": "~2.0",
    "caffeinated/modules": "2.1.*",
    "caffeinated/plugins": "~2.0",
    "caffeinated/shinobi": "~2.0",
    "caffeinated/themes": "~2.0",
    "caffeinated/widgets": "~2.0",
    "cviebrock/eloquent-sluggable": "^3.1",
    "doctrine/dbal": "^2.5",
    "filp/whoops": "^1.0",
    "genealabs/laravel-caffeine": "~0.2.0",
    "illuminate3/kotoba": "dev-master",
    "laracasts/presenter": "dev-master",
    "laravel/framework": "5.1.*",
    "laravel/socialite": "~2.0",
    "laravelcollective/html": "5.1.*",
    "laravelrus/localized-carbon": "dev-master",
    "opauth/facebook": "*",
    "opauth/google": "*",
    "opauth/opauth": "*",
    "opauth/twitter": "*",
    "rakko/core": "*",
    "rakko/kagi": "*",
    "rakko/kantoku": "*",
    "rakko/menus": "*",
    "rakko/origami": "*",
    "smarch/watchtower": "v1.1.5.4",
    "wikimedia/composer-merge-plugin": "^1.2",
    "yajra/laravel-datatables-oracle": "5.12.5"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "barryvdh/laravel-ide-helper": "^2.1",
    "barryvdh/laravel-debugbar": "^2.0"
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "app/Modules/*/composer.json"
      ]
    },
    "installer-paths": {
      "App/Modules/{$name}": [
        "rakko/core",
        "rakko/origami",
        "rakko/kantoku",
        "rakko/menus",
        "rakko/kagi"
      ],
      "resources/themes/{$name}": [
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/illuminate3/core"
    },
    {
      "type": "vcs",
      "url": "https://github.com/illuminate3/origami"
    },
    {
      "type": "vcs",
      "url": "https://github.com/illuminate3/kantoku"
    },
    {
      "type": "vcs",
      "url": "https://github.com/illuminate3/menus"
    },
    {
      "type": "vcs",
      "url": "https://github.com/illuminate3/kagi"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  }
}
